class Student {
    String name;
    int id;
    public void printInfo(){
        System.out.println(this.name);
        System.out.println(this.id);
    }
    public static void main (String args[]){
    
        Student s1=new Student();
        s1.name="Jim";
        s1.id=2410;
        Student s2=new Student();
        s2.name="Joty";
        s2.id=2345;
        s1.printInfo();
        s2.printInfo();
    }
    
}
